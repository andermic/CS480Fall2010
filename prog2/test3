function isqrt (r : int) : int
var low : int;
var high : int;
var mid : int;
begin
	printStr("starting");
	low = 0;
	high = r;
	while (high != low) 
	begin
		printStr("values");
		mid = (low + high) / 2;
		printInt(mid);
		if ((mid * mid) > r) 
			high = mid
		else
			low = mid;
	end;
	printStr("returning");
	return (low);
end;

function main ()
begin
	printInt(isqrt(104));
end;
